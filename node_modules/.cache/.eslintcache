n            alert(\"An error occured while fetching user data\");\r\n        }\r\n    };\r\n\r\n    useEffect(() => {\r\n        if (loading) return;\r\n        if (!user) return navigate(\"/login\");\r\n\r\n        fetchUserName();\r\n    }, [user, loading]);\r\n    \r\n    return (\r\n        <div>\r\n            <main className=\"user-dashboard\">\r\n                <div className=\"user-container\">\r\n                <div className=\"user-information row\">\r\n                    <img src alt=\"\" className=\"user-pic\" />\r\n                    <div className=\"user-details\">\r\n                    <p className=\"user-name\">{name}</p>\r\n                    <p className=\"birth-date\">Birth Date:</p>\r\n                    <p className=\"age\">Age:</p>\r\n                    <p className=\"id-num\">ID No.:</p>\r\n                    <p className=\"condition\">Condition:</p>\r\n                    <button onClick={logout} className=\"px-2 py-2 rounded-lg bg-[#F5C4C4]\">Sign Out (Temp)</button>\r\n                    </div>\r\n                </div>\r\n                <h3 className=\"center\">Records</h3>\r\n                <div className=\"user-records row\">\r\n                    <button type=\"submit\" className=\"gen-button\" id>mm/dd/yy</button>\r\n                    <button type=\"submit\" className=\"gen-button\" id>mm/dd/yy</button>\r\n                    <button type=\"submit\" className=\"gen-button\" id>mm/dd/yy</button>\r\n                </div>\r\n                </div>\r\n            </main>\r\n        </div>\r\n    );\r\n}\r\n","C:\\Users\\Nathalie\\Documents\\GitHub\\kineblitz\\src\\Pages\\Record.js",[],[],"C:\\Users\\Nathalie\\Documents\\GitHub\\kineblitz\\src\\firebase_setup\\firebase.js",[],[],"C:\\Users\\Nathalie\\Documents\\GitHub\\kineblitz\\src\\Pages\\Reset.js",["78","79"],[],"import React, { useEffect, useState } from 'react';\r\nimport hallway from '../Images/Hallway_Background.jpg';\r\nimport { useAuthState } from 'react-firebase-